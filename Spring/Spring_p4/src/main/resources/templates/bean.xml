<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="emp_p4" class="com.example.spring_p4.emp_p4">
    <property name="id" value="05"></property>
    <property name="name" value="Saurabh"></property>
</bean>
    <bean id="aob" class="com.example.spring_p4.Account">
        <property name="eob" ref="emp_p4"></property>
    </bean>
</beans>